{
  "name": "HbbTV",
  "ingestConfiguration": {
    "pull": true,
    "protocol": "urn:3gpp:5gms:content-protocol:http-pull-ingest",
    "entryPoint": "https://atv-hbb.iptv-playoutcenter.de/"
  },
  "distributionConfigurations": [
    {
      "canonicalDomainName": "localhost",
      "domainNameAlias": "10.0.2.2",
      "pathRewriteRules": [
	{
	  "requestPattern": "^/m4d/provisioning-session-[^/]*/",
	  "mappedPath": "/atv-hbb/augsburgtv.stream_all/"
	}
      ]
    }
  ]
}
